#!/home/jon/local/bin/perl -w

use strict;
use lib '../lib';

$| = 1;

use Data::Dumper;

use Sentinel;

my $s = Sentinel->new( config_file => 'bin/sentinel.cfg' );
$s->init_task_list();
$s->run();

print "debug is :: " . $s->debug() . "\n";
print "max_concurrent is :: " . $s->max_concurrent() . "\n";

exit 0;
